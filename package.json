{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "npm run lint:format && npm run lint:fix ",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "axios": "^0.24.0",
    "hamburger-react": "^2.5.0",
    "react": "^17.0.2",
    "react-burger-menu": "^3.0.8",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-hide-on-scroll": "^1.1.4",
    "react-icons": "^4.4.0",
    "react-js-stickynav": "^1.0.2",
    "react-player": "^2.10.1",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^6.2.1",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.47",
    "@types/react-burger-menu": "^2.8.3",
    "@types/react-dom": "^17.0.17",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "typescript": "^4.7.4",
    "vite": "^2.4.1"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "description": "A starter for React with Typescript with the fast Vite and all static code testing with Eslint and formatting with Prettier",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexis92140/Brille-Front.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/alexis92140/Brille-Front/issues"
  },
  "homepage": "https://github.com/alexis92140/Brille-Front#readme"
}
